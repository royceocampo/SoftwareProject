	public void open() {
		Display display = Display.getDefault();
		createContents();
		shlOrderManage.open();
		shlOrderManage.layout();
		while (!shell.isDisposed()) {
			if (!display.readAndDispatch()) {
				display.sleep();
			}
		}
	}
